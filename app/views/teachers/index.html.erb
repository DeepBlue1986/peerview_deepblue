<div class="container">
  <div class="row">
    <h1>Teachers</h1>
    <div class="col-xs-12 col-md-4-offset-3">
      <% @teachers.each do |teacher| %>
      <h6><strong>Name:</strong> <%= teacher.first_name %> <%= teacher.last_name %></h6>
      <h6><strong>Email:</strong> <%= teacher.email %></h6>
      <h6><strong>City:</strong> <%= teacher.city %></h6>
      <h6><strong>Description:</strong> <%= teacher.description %></h6>
      <hr>
      <% end %>
    </div>
  </div>
</div>
